{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "Attribute",
    "title": "Attribute",
    "type": "object",
    "properties": {
        "hasAttributeModifier": {
            "type": "array",
            "items": {
                "$ref": "AttributeModifier.json"
            },
            "default": []
        },
        "attributeName": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "minItems": 1
        }
    },
    "required": [
        "attributeName"
    ]
}